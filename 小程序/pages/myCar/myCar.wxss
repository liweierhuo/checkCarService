/* pages/myCar/myCar.wxss */
.container{
  border-top: 3px solid #99d6f5;
}
.carList{
  padding: 50rpx 50rpx 100rpx 50rpx
}
.carItem{
  background-color: #fff;
  border: 7px solid #98b9e6;
  padding: 30rpx;
  margin-bottom: 50rpx
}
.carNumber{
  text-align: center;
  line-height: 200rpx;
  font-size: 34rpx;
  color: #000
}
.carBts{
  width: 390rpx;
  margin: 0 auto;
  display: flex;
  justify-content: space-between
}
.bt1{
  width: 155rpx;
  height: 50rpx;
  text-align: center;
  line-height: 50rpx;
  background-color: #e5e5e5;
  border-radius: 5px
}
.bt2{
  width: 155rpx;
  height: 50rpx;
  text-align: center;
  line-height: 50rpx;
  background-color: #ff0000;
  color: #fff;
  border-radius: 5px
}
.addCar{
  background-color: #00b7ee;
  color: #fff;
  text-align: center;
  position: fixed;
  bottom: 0;
  right: 0;
  width: 100%;
  height: 100rpx;
  line-height: 100rpx
}
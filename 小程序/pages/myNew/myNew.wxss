}

.container {
  background-color: #98d1f5;
  min-height: 100%;
  padding-bottom: 10px;
}

.newList {
  margin: 0 40rpx;
  background-color: #fff;
  border-radius: 5px;
  padding: 0 40rpx;
}

.newItem:first-child {

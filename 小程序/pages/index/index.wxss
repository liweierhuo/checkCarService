.nav{margin-top: 150rpx;padding: 0 50rpx}
.navBorder{
border-bottom: 1px solid #bfbfbf;display:flex;flex-direction:row;
}
.default{position: relative;line-height:100rpx;text-align:center;flex:1;color:#838383;font-size:32rpx;}
.navTitleShow{position: relative;line-height:100rpx;text-align:center;color:#99c4f5;flex:1;font-size:32rpxï¼›}
.show{display:block;  margin-bottom: 120rpx}
.hidden{display:none; margin-bottom: 120rpx }
.tabborder{
  position: absolute;
  width: 50rpx;
  height: 5rpx;
  background-color: #689fff;
  bottom: 0;
  left: 50%;
